<h1>Orders</h1>

<table class="table">
	<thead>
		<tr>
			<th>Total</th>
			<th>Status</th>
			<th>Created at</th>
			<th>Updated at</th>
		</tr>
	</thead>
	<tbody>
		<% @orders.each do |order| %>
			<tr>
				<td><%= order.total %></td>
				<td><%= order.status %></td>
				<td><%= order.created_at %></td>
				<td><%= order.updated_at %></td>
			</tr>
		<% end %>
	</tbody>
</table>